@model IEnumerable<Mooshak2.Models.ViewModels.TeachersViewModel>
@{
    ViewBag.Title = "Teachers";
}

<h2>Teachers</h2>
<ol class="breadcrumb">
    <li><a href="~/Home">Home</a></li>
    <li><a href="~/Course">Course</a></li>
    <li class="active">Students</li>
</ol>

<div class="row">

    <div class="col-md-12">
        @if (!(Model.Count() > 0))
        {
            <div class="alert alert-info">You have no grades in this course</div>
        }

        @foreach (var item in Model)
        {
            <div>@item.Name.UserName</div>
        }

    </div>



</div>