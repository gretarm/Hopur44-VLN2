@model Mooshak2.Models.ViewModels.StudentViewModel
    @using Microsoft.AspNet.Identity

@{
    ViewBag.Title = "Index";
}

<h2>Welcome back, @User.Identity.GetUserName()</h2>

<div class="mainContent body-content col-md-¨4">
    <h2>Student</h2>
    @foreach (var item in Model.Courses)
    {
        @Html.ActionLink(item.Title + "\u00BB", "../Course/ID", new { id = item.CourseID }, new { @class = "btn btn-default menuButton" })
    }
</div>
